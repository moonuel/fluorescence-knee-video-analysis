    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["normal", "0308", "64", "--cycles", "1", "--metric", "flux", "--no-normalize", "--no-preview", "--flux-smoothing", "butter:0.25", "--flux-show-unsmoothed"]
        }
    ]
}